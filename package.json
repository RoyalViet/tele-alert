{
  "name": "telegram-alert",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "dev": "nodemon",
    "push": "git add --all && git commit -a -m \"update\" && git push",
    "dev:old": "nodemon --watch 'src/**' --ext 'ts,json' --ignore 'src/**/*.spec.ts' --exec 'ts-node src/server.ts'",
    "watch-ts": "tsc -w",
    "dev:node": "node --watch dist/server.js",
    "dev:tsc": "tsc --watch --preserveWatchOutput",
    "ts.check": "tsc --project tsconfig.json",
    "add-build": "git add dist",
    "build": "rimraf dist && tsc",
    "start": "node dist/server.js",
    "migration:generate": "typeorm migration:create -n",
    "migration:run": "ts-node ./node_modules/typeorm/cli.js migration:run",
    "migration:revert": "ts-node ./node_modules/typeorm/cli.js migration:revert"
  },
  "pre-commit": [
    "ts.check",
    "build",
    "add-build"
  ],
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "axios": "^1.7.7",
    "bcrypt": "^5.1.1",
    "bignumber.js": "^9.1.2",
    "body-parser": "^1.20.2",
    "cors": "^2.8.5",
    "cron": "^3.1.7",
    "dotenv": "^8.2.0",
    "ejs": "^3.1.3",
    "express": "^4.17.1",
    "fs": "0.0.1-security",
    "http-status-codes": "^2.3.0",
    "joi": "^17.13.1",
    "jsonwebtoken": "^9.0.2",
    "module-alias": "^2.2.3",
    "moment": "^2.30.1",
    "morgan": "^1.10.0",
    "mysql": "^2.18.1",
    "node-fetch": "^2.7.0",
    "path": "^0.12.7",
    "reflect-metadata": "^0.2.2",
    "typeorm": "^0.2.37",
    "winston": "^3.13.0"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.2",
    "@types/body-parser": "^1.19.5",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/morgan": "^1.9.9",
    "@types/mysql": "^2.15.26",
    "@types/node": "^20.12.11",
    "@types/node-fetch": "^2.6.11",
    "nodemon": "^3.1.0",
    "pre-commit": "^1.2.2",
    "rimraf": "^5.0.7",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.4.5"
  }
}
