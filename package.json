{
  "name": "telegram-alert",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "dev": "nodemon --watch 'src/**' --ext 'ts,json' --ignore 'src/**/*.spec.ts' --exec 'ts-node src/server.ts'",
    "dev:node": "node --watch dist/server.js",
    "dev:tsc": "tsc --watch --preserveWatchOutput",
    "start": "node dist/server.js",
    "ts.check": "tsc --project tsconfig.json",
    "add-build": "git add dist",
    "build": "rimraf dist && tsc"
  },
  "pre-commit": [
    "ts.check",
    "build",
    "add-build"
  ],
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "axios": "^1.6.8",
    "bignumber.js": "^9.1.2",
    "body-parser": "^1.20.2",
    "cors": "^2.8.5",
    "cron": "^3.1.7",
    "dotenv": "^8.2.0",
    "ejs": "^3.1.3",
    "express": "^4.17.1",
    "heroku": "^8.11.5",
    "mysql2": "^3.9.7"
  },
  "devDependencies": {
    "@types/body-parser": "^1.19.5",
    "@types/express": "^4.17.21",
    "@types/mysql": "^2.15.26",
    "@types/node": "^20.12.11",
    "nodemon": "^3.1.0",
    "pre-commit": "^1.2.2",
    "rimraf": "^5.0.7",
    "typescript": "^5.4.5"
  }
}
